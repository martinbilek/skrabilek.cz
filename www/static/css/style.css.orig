/** RESET **/
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, font, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td {
	margin: 0;
	padding: 0;
	border: 0;
	outline: 0;
	font-size: 100%;
	vertical-align: baseline;
	background: transparent;
}
body {
	line-height: 1;
}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after,
q:before, q:after {
	content: '';
	content: none;
}
:focus {
	outline: 0;
}
ins {
	text-decoration: none;
}
del {
	text-decoration: line-through;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
/** KONEC RESET **/
html {
	font-family: Trebuchet MS, Tahoma;
	font-size:13px;
	background:url(../img/contentGrad.gif);
}

a:hover,a:focus,a:active {
	text-decoration:none;
}

#head {
  height:200px;
	background:url(../img/headGrad.gif) repeat-x;
}
#logo {
  padding:61px 0 0 0;
	height:139px;
	width:1054px;
	margin:0 auto 0 auto;
	background:url(../img/headDecoration.gif) right no-repeat;
}
/** MENU **/
#menu {
	background:url(../img/menuGrad.gif) repeat-x;
	height:38px;
}
#menu ul {
	width:790px;
	margin:0 auto 0 auto;
	height:38px;
  padding:4px 0 0 0;
}
#menu ul li {
	display:block;
	float:left;
	margin:0 3px 0 3px;
}
#menu ul li a {
	height:30px;
	line-height:30px;
	display:block;
  text-transform: uppercase;
	font-weight:bold;
	color:#fff;
	text-decoration:none;
  cursor: hand;
	_float:left;
}
#menu ul li a span {
  display: block;
  height:30px;
	_float:left;
}
#menu ul li a span span {
  padding: 0 15px 0 15px;
  height:30px;
}
#menu ul li a:hover,#menu ul li a:focus,#menu ul li a:active {
  background:url(../img/menuInit.gif) repeat-x;
}
#menu ul li a:hover span,#menu ul li a:focus span,#menu ul li a:active span {
  background:url(../img/menuInitLeft.gif) left no-repeat;
}
#menu ul li a:hover span span,#menu ul li a:focus span span,#menu ul li a:active span span {
  background:url(../img/menuInitRight.gif) right no-repeat;
}
/** KONEC MENU **/
