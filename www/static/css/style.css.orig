/** OSTATNI **/
.floatLeft {
  float:left;
}
.floatRight {
  float:right;
}
.blind {
	position:absolute;
	left:-10000px;
	margin:-1000px 0 0 0;
	width:1px;
	height:1px;
}
.reset {
	border:0 none;
	clear:both;
	float:none;
	font-size:0;
	height:0;
	line-height:0;
	margin:0;
	padding:0;
	width:0;
}
